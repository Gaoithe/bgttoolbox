http://ubuntuforums.org/showthread.php?p=5734480

# to mount
sudo mkdir /mnt/Zen-Xfi
sudo chmod 777 /mnt/Zen-Xfi

sudo mtpfs /mnt/Zen-Xfi -o allow_other

# to unmount:
sudo fusermount -u /mnt/Zen-Xfi


